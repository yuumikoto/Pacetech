package calculos;

public class Usuario 
{
    private float peso, altura;

    
    public float imc() 
    {
        float imc = peso / (altura * altura);
        return imc;
    }

    public void setPeso(float peso) 
    {        
        this.peso = peso;
    }
    
    public float getPeso()
    {
        return peso;
    }
    
    public void setAltura(float altura)
    {
        this.altura = altura;
    }
    
    public float getAltura()
    {
        return altura;
    }
}